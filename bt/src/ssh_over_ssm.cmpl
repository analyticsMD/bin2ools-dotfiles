_complete_ssh_hosts () {
        COMPREPLY=()
        cur="${COMP_WORDS[COMP_CWORD]}"
        comp_ssh_hosts=`cat ${BT}/config.d/ssh_over_ssm.conf | grep "^\s*Host " | awk '{print $2}'`
        COMPREPLY=( $(compgen -W "${comp_ssh_hosts}" -- $cur))
        return 0
} 
complete -F _complete_ssh_hosts ssh_over_ssm

